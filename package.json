{
  "name": "prostokit-react-css",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\" index.html vite.config.ts",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\" index.html vite.config.ts"
    "audit": "npm audit"
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "lucide-react": "0.460.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.0",
    "@eslint/js": "^9.10.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.0.0"
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "typescript": "5.2.2",
    "vite": "5.0.0",
    "@vitejs/plugin-react": "4.2.0"
  }
}